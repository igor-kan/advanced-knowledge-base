{
  "name": "advanced-knowledge-base",
  "version": "1.0.0",
  "description": "High-performance knowledge base implementations with sophisticated relationship modeling",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:setup": "npm run docker:up && npm run db:migrate",
    "dev:start": "concurrently \"npm run dev\" \"npm run api:start\"",
    "build": "npm run build:neo4j && npm run build:custom && npm run build:distributed",
    "build:neo4j": "cd implementations/neo4j-graph && npm run build",
    "build:custom": "cd implementations/custom-engine && npm run build",
    "build:distributed": "cd implementations/distributed && npm run build",
    "test": "jest",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:performance": "jest --testPathPattern=performance --runInBand",
    "benchmark": "node benchmarks/runner.js",
    "benchmark:all": "npm run benchmark:insert && npm run benchmark:query && npm run benchmark:memory",
    "benchmark:insert": "node benchmarks/insert-performance.js",
    "benchmark:query": "node benchmarks/query-performance.js",
    "benchmark:memory": "node benchmarks/memory-usage.js",
    "benchmark:scale": "node benchmarks/scalability.js",
    "loadtest:nodes": "node loadtests/billion-nodes.js",
    "loadtest:edges": "node loadtests/billion-edges.js",
    "loadtest:queries": "node loadtests/concurrent-queries.js",
    "stress:memory": "node stresstests/memory-exhaustion.js",
    "stress:cpu": "node stresstests/cpu-saturation.js",
    "stress:network": "node stresstests/network-bandwidth.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "db:migrate": "node scripts/migrate-databases.js",
    "db:seed": "node scripts/seed-test-data.js",
    "api:start": "node src/api/server.js",
    "api:graphql": "node src/api/graphql-server.js",
    "api:websocket": "node src/api/websocket-server.js",
    "monitor": "node src/monitoring/dashboard.js",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "docs:generate": "jsdoc src/ -r -d docs/",
    "docs:serve": "http-server docs/ -p 8081"
  },
  "keywords": [
    "knowledge-base",
    "graph-database",
    "relationships",
    "semantic-web",
    "high-performance",
    "distributed-systems",
    "big-data",
    "machine-learning",
    "artificial-intelligence"
  ],
  "author": "Igor Kan <igor@example.com>",
  "license": "MIT",
  "dependencies": {
    "neo4j-driver": "^5.15.0",
    "redis": "^4.6.12",
    "redisgraph": "^2.5.1",
    "apache-jena": "^4.10.0",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "apollo-server-express": "^3.12.1",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "mongoose": "^8.0.3",
    "elasticsearch": "^16.7.3",
    "cassandra-driver": "^4.7.2",
    "leveldb": "^6.2.1",
    "rocksdb": "^5.4.2",
    "lmdb": "^2.9.2",
    "msgpack": "^1.0.3",
    "snappy": "^7.2.2",
    "lz4": "^0.6.5",
    "xxhash": "^0.3.0",
    "murmurhash": "^2.0.1",
    "bloom-filters": "^3.0.1",
    "hyperloglog": "^1.4.0",
    "roaring": "^0.9.1",
    "sparse-bitfield": "^3.0.3",
    "avl": "^1.4.4",
    "rb-tree": "^1.0.1",
    "async": "^3.2.5",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.4",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "pino": "^8.17.1",
    "dotenv": "^16.3.1",
    "config": "^3.3.9"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jsdoc": "^4.0.2",
    "http-server": "^14.1.1",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "benchmark": "^2.1.4",
    "clinic": "^12.1.0",
    "0x": "^5.5.0",
    "artillery": "^2.0.3",
    "k6": "^0.47.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/igor-kan/advanced-knowledge-base.git"
  },
  "bugs": {
    "url": "https://github.com/igor-kan/advanced-knowledge-base/issues"
  },
  "homepage": "https://github.com/igor-kan/advanced-knowledge-base#readme"
}